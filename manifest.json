{
  "manifest_version": 3,
  "name": "Custom Menu",
  "version": "1.0",
  "description": "Add a custom menu to your browser toolbar.",
  "homepage_url": "https://openaddons.com/custom-menu/",

  "icons":{"128": "icon128.png"},
  "permissions": [

  ],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "128": "icon128.png"
    }
  },
  "background": {
    "service_worker": "background.js"
  }
}
